{"ast":null,"code":"import { getMovieList } from \"@/servies/modules/movie_list\";\nimport { ref } from \"vue\";\n\n//请求接口数据\n\nexport default {\n  __name: 'movieList',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const movieList = ref([]); // 创建movieList变量存储下面拿到的res数据\n    getMovieList().then(res => {\n      //把请求到的数据存起来\n      movieList.value = res.data.films; //这个movielist是对象，所以是存到它里面的value里面\n      console.log(movieList);\n    });\n    const __returned__ = {\n      movieList,\n      get getMovieList() {\n        return getMovieList;\n      },\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["getMovieList","ref","movieList","then","res","value","data","films","console","log"],"sources":["/Users/nicolasroy/Desktop/卖座网实战/maizuo_movie/src/components/movieList.vue"],"sourcesContent":["<template>\n  <div class=\"movieList\">\n    <template v-for=\"(item, index) in movieList\" :key=\"index\">\n      <!-- <template v-if=\"item.actors.role === '演员'\"> -->\n        <div class=\"movie-item\">\n        <div class=\"cover\">\n          <img :src=\"item.poster\" alt=\"\" />\n        </div>\n        <div class=\"item-info\">\n          <div class=\"tittle\">{{ item.name }}</div>\n          <div class=\"score\">观众评分 {{ item.grade }}</div>\n          <div class=\"director\">导演：{{ item.director }}</div>\n          <div class=\"actor\">\n            演员：\n            <template v-for=\"iten in item.actors\" :key=\"index\" >\n              {{ iten.name }}\n            </template>\n          </div>\n        </div>\n        <div class=\"item-buybtn\"><button>购票</button></div>\n      </div>\n      <!-- </template> -->\n    </template>\n  </div>\n</template>\n\n<script setup>\nimport { getMovieList } from \"@/servies/modules/movie_list\"\nimport { ref } from \"vue\"\n\n//请求接口数据\nconst movieList = ref([]) // 创建movieList变量存储下面拿到的res数据\ngetMovieList().then(res => {\n  //把请求到的数据存起来\n  movieList.value = res.data.films //这个movielist是对象，所以是存到它里面的value里面\n  console.log(movieList)\n})\n</script>\n\n<style lang=\"scss\" scoped>\n.movieList {\n  .movie-item {\n    display: flex;\n    img {\n      width: 4rem;\n      height: 5.625rem;\n    }\n  }\n}\n</style>\n@/servies/axios_request/index @/servies/new_class_axios\n"],"mappings":"AA2BA,SAASA,YAAY,QAAQ,8BAA8B;AAC3D,SAASC,GAAG,QAAQ,KAAK;;AAEzB;;;;;;;;IACA,MAAMC,SAAS,GAAGD,GAAG,CAAC,EAAE,CAAC,EAAC;IAC1BD,YAAY,CAAC,CAAC,CAACG,IAAI,CAACC,GAAG,IAAI;MACzB;MACAF,SAAS,CAACG,KAAK,GAAGD,GAAG,CAACE,IAAI,CAACC,KAAK,EAAC;MACjCC,OAAO,CAACC,GAAG,CAACP,SAAS,CAAC;IACxB,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}