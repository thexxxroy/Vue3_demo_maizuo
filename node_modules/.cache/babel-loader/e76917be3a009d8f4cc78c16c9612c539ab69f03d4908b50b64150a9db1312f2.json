{"ast":null,"code":"import { getMovieList } from \"@/servies/modules/movie_list\";\nimport { ref } from \"vue\";\n\n//请求接口数据\n\nexport default {\n  __name: 'movieList',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const movieList = ref([]); // 创建movieList变量存储下面拿到的res数据\n    getMovieList().then(res => {\n      //把请求到的数据存起来\n      movieList.value = res.data.films; //这个movielist是对象，所以是存到它里面的value里面\n      console.log(movieList);\n    });\n    const __returned__ = {\n      movieList,\n      get getMovieList() {\n        return getMovieList;\n      },\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["getMovieList","ref","movieList","then","res","value","data","films","console","log"],"sources":["/Users/nicolasroy/Desktop/卖座网实战/maizuo_movie/src/components/movieList.vue"],"sourcesContent":["<template>\n  <div class=\"movieList\">\n    <template v-for=\"(item, index) in movieList\" :key=\"index\">\n      <div class=\"movie-item\">\n        <div class=\"cover\">\n          <img :src=\"item.poster\" alt=\"\" />\n        </div>\n\n        <div class=\"item-info\">\n          <div class=\"tittle\">{{ item.name }}</div>\n          <div class=\"info\">\n            <div class=\"score\">\n              观众评分\n              <div class=\"star\">{{ item.grade }}</div>\n            </div>\n            <div class=\"director\">导演：{{ item.director }}</div>\n            <div class=\"actors\">\n              演员：\n              <template v-for=\"iten in item.actors\" :key=\"index\">\n                <span class=\"actor\" v-show=\"iten.role !== '导演'\">\n                  {{ iten.name }}&nbsp;\n                </span>\n              </template>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"buybtn\">\n          <button>购票</button>\n        </div>\n      </div>\n    </template>\n  </div>\n</template>\n\n<script setup>\nimport { getMovieList } from \"@/servies/modules/movie_list\"\nimport { ref } from \"vue\"\n\n//请求接口数据\nconst movieList = ref([]) // 创建movieList变量存储下面拿到的res数据\ngetMovieList().then(res => {\n  //把请求到的数据存起来\n  movieList.value = res.data.films //这个movielist是对象，所以是存到它里面的value里面\n  console.log(movieList)\n})\n</script>\n\n<style lang=\"scss\" scoped>\n.movieList {\n  margin: 0.3125rem;\n  .movie-item {\n    display: flex;\n    align-items: center;\n    margin: 5px;\n    .cover {\n      img {\n        width: 4rem;\n        height: 5.625rem;\n      }\n    }\n    .item-info {\n      // margin: 5px;\n      // margin-left: 5px;\n      padding: 0 10px;\n      .tittle {\n        font-size: 14px;\n        margin-bottom: 5px;\n      }\n      .info {\n        font-size: 0.12rem;\n        color: #8c8c8c;\n        .score {\n          margin-bottom: 3px;\n          // margin: 3px 0;\n          display: flex;\n          .star {\n            margin-left: 2px;\n          font-weight: 500;\n            color: #ff5f16;\n          }\n        }\n        .director {\n          margin-bottom: 3px;\n\n        }\n        .actors {\n          display: flex;\n          width: 14rem;\n          margin-bottom: 3px;\n          white-space: nowrap; //文本不会换行\n          text-overflow: ellipsis; //文本溢出显示省略号\n          overflow: hidden;\n        }\n      }\n    }\n\n    .buybtn {\n      display: flex;\n      color: orange;\n      width: 56px;\nalign-items: center;\njustify-content: center;\n    }\n  }\n}\n</style>\n\n@/servies/axios_request/index @/servies/new_class_axios\n"],"mappings":"AAoCA,SAASA,YAAY,QAAQ,8BAA8B;AAC3D,SAASC,GAAG,QAAQ,KAAK;;AAEzB;;;;;;;;IACA,MAAMC,SAAS,GAAGD,GAAG,CAAC,EAAE,CAAC,EAAC;IAC1BD,YAAY,CAAC,CAAC,CAACG,IAAI,CAACC,GAAG,IAAI;MACzB;MACAF,SAAS,CAACG,KAAK,GAAGD,GAAG,CAACE,IAAI,CAACC,KAAK,EAAC;MACjCC,OAAO,CAACC,GAAG,CAACP,SAAS,CAAC;IACxB,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}